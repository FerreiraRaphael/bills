{
  "name": "bills",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "fastapi-dev": "pip install -r requirements.txt && python -m uvicorn api.index:app --reload --host 0.0.0.0 --port 8000",
    "next-dev": "next dev --turbo",
    "dev": "concurrently \"npm run next-dev\" \"npm run fastapi-dev\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@clerk/nextjs": "^4.29.12",
    "concurrently": "^8.2.2",
    "next": "14.1.4",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.4",
    "postcss": "^8",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
